{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "ES6",
    "jsx": "react",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "allowJs": true,
    "sourceMap": true,
    "outDir": "./dist",
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
